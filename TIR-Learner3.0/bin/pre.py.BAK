from Bio import SeqIO


def check_sequence_names(genome_file):
    records = SeqIO.parse(genome_file, "fasta")
    names = [rec.id for rec in records]
    for name in names:
        if "-+-" in names:
            print("sequence name has special character")
        c = names.count(name)
        if c > 1:
            print(name + " presents" + str(c) + " times in the genome file")
    print(str(len(names)) + " sequences in genome file")
